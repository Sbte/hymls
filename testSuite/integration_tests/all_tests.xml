  <Parameter name="Test 6a" type="string" value="stokes3.xml"/>
  <Parameter name="Test 6b" type="string" value="stokes4.xml"/>
  <Parameter name="Test 6c" type="string" value="stokes4_3D.xml"/>
  <!-- Direct solver multiple levels -->
  <Parameter name="Test 7" type="string" value="stokes5.xml"/>
  <!-- Extra retained nodes -->
  <Parameter name="Test 8" type="string" value="stokes6.xml"/>
  <!-- Stokes on a B-grid -->
  <Parameter name="Test 9a" type="string" value="stokes_B.xml"/>
  <Parameter name="Test 9b" type="string" value="stokes_THCM.xml"/>
</ParameterList>
