  <Parameter name="3D Test 2" type="string" value="stokes0_3D.xml"/>
  <Parameter name="3D Test 3" type="string" value="stokes1_3D.xml"/>
  <Parameter name="3D Test 4" type="string" value="stokes2_3D.xml"/>
